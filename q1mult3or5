#include <iostream>


int main(int argc, const char * argv[])
{
    // insert code here...
  int mult3x5sum = 0;
  for (int i = 0; i<1000; i++) {
    if ( ( (i%5)==0) || ( (i % 3) == 0) ) {
      mult3x5sum = mult3x5sum + i;
      
    }
  }
  printf("Below 1000 Multiples of 3 and 5 summation\n %d \n",  mult3x5sum);
  std::cout << "Hello, Worldd!\n";
  return 0;

}
